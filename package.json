  },
  "devDependencies": {
    "@babel/cli": "^7.28.3",
    "@types/node": "25.0.3",
    "babel-plugin-i18next-extract": "^1.1.0",
    "babel-plugin-root-import": "^6.6.0",
    "prettier": "3.7.4",
    "rimraf": "^6.1.2"
